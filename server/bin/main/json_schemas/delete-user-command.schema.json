{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "password": {
      "type": "string",
      "minLength": 8,
      "maxLength": 32,
      "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[@$!%*?&])[A-Za-z\\d@$!%*?&]{8,32}$",
      "description": "Debe coincidir con el formato: 8-32 caracteres, mayúscula, minúscula, número y especial."
    }
  },
  "required": ["password"],
  "additionalProperties": false
}